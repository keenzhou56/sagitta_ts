{
  "compilerOptions": {
    "rootDir": "./src_ts",
    "outDir": "./src",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": false,
    "strictNullChecks": true,
    "traceResolution": true,
    "allowJs": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react"
  },
  "include": [
    "./src_ts/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
 // "files": ["./index.ts"]
}